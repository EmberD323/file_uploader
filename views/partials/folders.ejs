<ul>
    <%- include("./errors.ejs") %>
    <% folders.forEach(function(folder) { %>
    <a href="/<%= folder.folder_name %>"><%= folder.folder_name %> </a> 
    <div>Created: <%= folder.createdAt %></div>
    <form action="/<%= folder.folder_name %>/renamefile" method="GET">
        <button type="submit" >Rename</button>
    </form> 
    <form action="/<%= folder.folder_name %>/deletefile" method="POST">
        <button type="submit" onclick="return confirm('Are you sure you want to delete this folder?');">Delete</button>
    </form>
    <% }); %>
</ul>
